    <div id="cntq" class="container m-5">

        <div class="row">
            <div class="col-2"></div>
            <div class="col-7 m-5 text-center">
                <h2>Here are the awesome Quotes!</h2>
                <br>
                <div class="row">
                    <% for (var cita of quotes) { %>
                        <h3>Author: <%= cita.who %></h3>
                        <h3>Quote: <%= cita.quote %></h3>
                        <hr>
                    <% } %> 
                </div> <!-- /row contenido-->
            </div>

            <div class="col-2"></div>
        </div> <!-- /row inicial-->
    </div> <!-- /container-->
